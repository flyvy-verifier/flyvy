sort thread
mutable p(thread): bool
mutable q(thread): bool
immutable t0: thread

def pq(t: thread) -> bool {
  p(t) & q(t)
}

def next() -> bool {
  (exists t:thread. pq(t)) & pq(t)' = pq(t) | t = t0
}

assume init
assume next()
assume !q
assert p(t0)
